<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.NurseProgram">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.NurseProgram.Views.TestInfoActivity"></activity>
        <activity android:name="com.example.NurseProgram.Views.PatientHistoryActivity" />
        <activity android:name="com.example.NurseProgram.Views.PatientEditActivity" />
        <activity
            android:name="com.example.NurseProgram.Views.PatientAddActivity"
            android:parentActivityName="com.example.NurseProgram.Views.PatientActivity" />
        <activity
            android:name="com.example.NurseProgram.Views.NurseRegistrationActivity"
            android:parentActivityName="com.example.NurseProgram.Views.MainActivity" />
        <activity android:name="com.example.NurseProgram.Views.ViewTestInfoActivity" />
        <activity android:name="com.example.NurseProgram.Views.TestActivity" />
        <activity android:name="com.example.NurseProgram.Views.PatientActivity" />
        <activity
            android:name="com.example.NurseProgram.Views.LoginActivity"
            android:parentActivityName="com.example.NurseProgram.Views.MainActivity" />
        <activity android:name="com.example.NurseProgram.Views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>